!
! setup w11 like processor configuration
!
set cpu 70
set cpu nofpp
set idle delay=1
! set sysid, leading '1' indicates simulator, next '2' e11
set cpu SYSID=0120345
!
! create log file, the only way to capture e11 output
! no rediction to stdout possible
!
log tt0: tmp_e11_tt0
